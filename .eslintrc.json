{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
  
    "extends": [
      "eslint:recommended",
    "plugin:react/recommended",   
    "prettier"
    ],
  
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "unicorn/number-literal-case": "off"
    }
  }